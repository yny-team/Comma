<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!-- DB snake_case => VO camelCase 매핑 -->
	<!-- user_id       => userId -->
	<!-- time_id       => timeId -->
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"></setting>
	</settings>
	
 	<typeAliases>
 	
		<typeAlias type="kr.or.comma.user.vo.UserVO" alias="UserVO"/>
		<typeAlias type="kr.or.comma.timeline.vo.TimelineVO" alias="TimelineVO"/>
		
		<typeAlias type="kr.or.comma.common.vo.ImageOrFileVO" alias="ImageOrFileVO"/>
			
		<typeAlias type="kr.or.comma.timeline.vo.TimelineCommentVO" alias="TimelineCommentVO"/>
		
		<typeAlias type="kr.or.comma.timeline.vo.TimelineLikeVO" alias="TimelineLikeVO"/>
		
		<typeAlias type="kr.or.comma.timeline.vo.TimelineWithImageVO" alias="TimelineWithImageVO"/>
	</typeAliases>
	
</configuration>
